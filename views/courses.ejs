<section id="bootcamp">
    <div class="container" id="first">
        <h1 class="text-center display-4"><a href="https://www.udemy.com/the-web-developer-bootcamp/">Web Dev Bootcamp</a> </h1>
        <hr>
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <ul class="text-center unbullet">
                        <li>NodeJS</li>
                        <li>jQuery</li>
                        <li>Intermediate HTML/CSS</li>
                    </ul>
                </div>
                <div class="col-lg-6">
                    <ul class="text-center unbullet">
                        <li>MongoDB</li>
                        <li>Express</li>
                        <li>Bootstrap</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row align-items-center bg-dark text-light">
